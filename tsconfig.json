{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"module": "None",
		"rootDir": "scripts/",
		"outDir": "scripts_dist/",
		"alwaysStrict": true,
		"isolatedModules": false,
		"allowJs": true,
		"strict": true,
		"noImplicitAny": false,
		"typeRoots": ["./scripts/types", "./scripts"],
		"types": ["p2ce"]
	},
	"include": ["scripts/"]
}
